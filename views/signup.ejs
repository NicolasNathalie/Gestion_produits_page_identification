<%- include('partials/header') %>
  <div class="container">

     <div class="col-6  m-auto"> 
<div class="data-form">
    <form class="form-signin" action="/signup" method="post">
      <h1 class="form-signin-heading text-center mt-5">Register</h1>
      <label for="inputName"></label>
      <input type="text" name="name" id="inputName" class="form-control" placeholder="Name" autofocus>
      <label for="inputEmail"></label>
      <input type="email" name="email" id="inputEmail" class="form-control" placeholder="Email">
      <label for="inputPassword"></label>
      <input type="password" name="password" id="inputPassword" class="form-control" placeholder="Password">
      
      <br /><button class="btn btn-lg btn-primary btn-block" type="submit">Register</button>
    </form>
    <%- include("partials/messages") -%>
    <a href="/login" class="link">Back to login</a>
  </div>
</div>
</div>
  
 









